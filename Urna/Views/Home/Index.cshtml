    </div>
    <div id="Teclado">
        <div id="fileira1">
            <button id="primeiro" onclick="Condition('1','1')">1</button>
            <button id="segundo" onclick="Condition('2','2')">2</button>
            <button id="terceiro" onclick="Condition('3','3')">3</button>
        </div>
        <div id="fileira2">
            <button id="terceiro" onclick="Condition('4','4')">4</button>
            <button id="quarto" onclick="Condition('5','5')">5</button>
            <button id="quinto" onclick="Condition('6','6')">6</button>
        </div>
        <div id="fileira3">
            <button onclick="Condition('7','7')">7</button>
            <button onclick="Condition('8','8')">8</button>
            <button onclick="Condition('9','9')">9</button>
        </div>

        <div id="fileira4">
            <button onclick="Condition('0','0')">0</button>
        </div>

        <div id="fileira5">
            <button id="branco">BRANCO</button>
            <button id="corrige" onclick="Limpar()">CORRIGE</button>
            <button id="confirma">CONFIRMA</button>
        </div>


    </div>
    <div id="Title">
        <h1 id="Justica">JUSTIÇA</h1>
        <h1 id="Eleitoral">ELEITORAL</h1>
        <img id="logo" src="~/img/image.png" alt="">
